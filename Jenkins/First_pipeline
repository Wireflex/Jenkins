    agent { 
        label 'master'
        }
    options {
        buildDiscarder(logRotator(numToKeepStr: '10', artifactNumToKeepStr: '10'))
        timestamps()
    }
    stages {
        stage("First step") {
            steps {
                sh 'ssh root@94.142.141.77  \'hostname\''
            }
        }
        stage("Second step") {
            steps {
                sh 'ssh root@94.142.141.77  \'uptime\''
            }
        }
    }
}
